    function SaveInfo() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var postData = $("#form1").GetWebControls(keyValue);
        //if ($("#TOWN").attr("data-text") != undefined) {
        //    postData.TOWNNAME = $("#TOWN").attr("data-text");
        //}
        //滑坡相对河流位置的值
        var one = "";
        var two = "";
        //滑坡相对河流位置的值
        $("input[name='POSITIONTORIVER1']").each(function () {
            if ($(this).prop("checked")) {
                one = $(this).val();
            }
        })
        $("input[name='POSITIONTORIVERall']").each(function () {
            if ($(this).prop("checked")) {
                two = $(this).val();
            }
        });
        if (postData.TOWN == "") {
            //var Validatemsg = "乡镇不能为空！\n";
            //ValidationMessage($("#TOWN"), Validatemsg); return false;
            dialogAlert('乡镇不能为空', 0);
            return false;
        }
        var dmtKey = postData.UNIFIEDCODE;
        postData.DATASOURCE = DATASOURCE;
        postData.POSITIONTORIVER = one + two;
        $.SaveForm({
            url: "../../JXGeoManage/TBL_LANDSLIP/SaveForm?keyValue=" + keyValue,
            param: postData,
            loading: "正在保存数据...",
            success: function (data) {
                if ($("#MultMedia").attr("src") != "") {
                    $("#MultMedia")[0].contentWindow.SaveFileInfo(dmtKey);
                }
                try {
                    getInfoTop().learun.currentIframe().$('#gridTable').trigger('reloadGrid');
                    getInfoTop().learun.currentIframe().BindMapMarker();
                } catch (e) {
                    window.parent.$('#gridTable').trigger('reloadGrid');
                }
            }
        })
    }
