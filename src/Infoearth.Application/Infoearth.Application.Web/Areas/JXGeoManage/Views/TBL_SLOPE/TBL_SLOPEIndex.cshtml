             type: "GET",
             success: function (data) {
                       var dataSet = JSON.parse(data);
                        mapCtlExt= $("#mapControl").mapCtl({
                                mapwidth: 1500,
                                isShowGisLayer:false,
                                isFrame:true,
                                dataset: dataSet,
                                isCluster: true
                             });
