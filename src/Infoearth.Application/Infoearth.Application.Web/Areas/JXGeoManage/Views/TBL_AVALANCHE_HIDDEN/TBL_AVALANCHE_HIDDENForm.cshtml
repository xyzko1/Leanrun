    function SetControl(data) {
        $("#PROVINCENAME").val(data.LOCATION.split(",")[0]);
        //$("#TOWNNAME").val(data.LOCATION.split(",")[3]);
        //$("#CITYNAME").val(data.LOCATION.split(",")[1] + "," + data.LOCATION.split(",")[2]);
        //读取双选radio状态
        if (data.POSITIONTORIVER == "") {
            $("input[name='POSITIONTORIVER1']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVER1']").eq(1).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(1).prop("checked", "")
        } else if (data.POSITIONTORIVER == "A   ") {
            $("input[name='POSITIONTORIVER1']").eq(0).prop("checked", "checked")
            $("input[name='POSITIONTORIVER1']").eq(1).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(1).prop("checked", "")
        } else if (data.POSITIONTORIVER == "B   ") {
            $("input[name='POSITIONTORIVER1']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVER1']").eq(1).prop("checked", "checked")
            $("input[name='POSITIONTORIVERall']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(1).prop("checked", "")
        } else if (data.POSITIONTORIVER == "C   ") {
            $("input[name='POSITIONTORIVER1']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVER1']").eq(1).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(0).prop("checked", "checked")
            $("input[name='POSITIONTORIVERall']").eq(1).prop("checked", "")
        } else if (data.POSITIONTORIVER == "D   ") {
            $("input[name='POSITIONTORIVER1']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVER1']").eq(1).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(1).prop("checked", "checked")
        } else if (data.POSITIONTORIVER == "AC  " || data.POSITIONTORIVER == "CA  ") {
            $("input[name='POSITIONTORIVER1']").eq(0).prop("checked", "checked")
            $("input[name='POSITIONTORIVER1']").eq(1).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(0).prop("checked", "checked")
            $("input[name='POSITIONTORIVERall']").eq(1).prop("checked", "")
        } else if (data.POSITIONTORIVER == "AD  " || data.POSITIONTORIVER == "DA  ") {
            $("input[name='POSITIONTORIVER1']").eq(0).prop("checked", "checked")
            $("input[name='POSITIONTORIVER1']").eq(1).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(1).prop("checked", "checked")
        } else if (data.POSITIONTORIVER == "BC  " || data.POSITIONTORIVER == "CB  ") {
            $("input[name='POSITIONTORIVER1']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVER1']").eq(1).prop("checked", "checked")
            $("input[name='POSITIONTORIVERall']").eq(0).prop("checked", "checked")
            $("input[name='POSITIONTORIVERall']").eq(1).prop("checked", "")
        } else if (data.POSITIONTORIVER == "BD  " || data.POSITIONTORIVER == "DB  ") {
            $("input[name='POSITIONTORIVER1']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVER1']").eq(1).prop("checked", "checked")
            $("input[name='POSITIONTORIVERall']").eq(0).prop("checked", "")
            $("input[name='POSITIONTORIVERall']").eq(1).prop("checked", "checked")
        }
        $("#form1").SetWebControls(data);
        //获取项目信息
        if (data && data.PROJECTID) {
            $.ajax({
                url: "../../api/JYGC_PROJECTBASEINFOApi/GetFormJson",
                data: { keyValue: data.PROJECTID },
                beforeSend: function (a) {
                    a.setRequestHeader("Authorization", authToken);
                },
                type: 'GET',
                dataType: "json",
                success: function (prj) {
                    var webData = {};
                    webData["PROJECTNAME"] = prj.PROJECTNAME;
                    $("#form1").SetWebControls(webData);
                },
                cache: false

            })
        }
    }

    //保存表单;
    function AcceptClick() {
        if (keyValue) {
            var queryJson = { 'unifycode': keyValue, "type": "08" }
            $.ajax({
                url: "../../api/TBL_AUDITINFOApi/GetPageNoAuditListJson",
                beforeSend: function (request) {
                    request.setRequestHeader("Authorization", authToken);
                },
                async: false,
                data: { queryJson: JSON.stringify(queryJson) },
                type: "get",
                dataType: "json",
                success: function (res) {
                    if (res.rows.length > 0) {
                        dialogMsg('该点存在未审核数据，请审核后再执行此操作！', 0);
                        return;
                    }
                },
            });
        }
        if (!$('#form1').Validform()) {
            return false;
        }
        var postData = $("#form1").GetWebControls(keyValue);
        //if ($("#VILLAGE").val() == "") {
        //    postData.LOCATION = postData.PROVINCENAME + "," + postData.CITYNAME + "," + postData.TOWNNAME;
        //} else if ($("#TEAM").val() == "") {
        //    postData.LOCATION = postData.PROVINCENAME + "," + postData.CITYNAME + "," + postData.TOWNNAME + "," + postData.VILLAGE;
        //} else {
        //    postData.LOCATION = postData.PROVINCENAME + "," + postData.CITYNAME + "," + postData.TOWNNAME + "," + postData.VILLAGE + "," + postData.TEAM;
        //}
        var one = "";
        var two = "";
        //滑坡相对河流位置的值
        $("input[name='POSITIONTORIVER1']").each(function () {
            if ($(this).prop("checked")) {
                one = $(this).val();
            }
        })
        $("input[name='POSITIONTORIVERall']").each(function () {
            if ($(this).prop("checked")) {
                two = $(this).val();
            }
        })
        postData.POSITIONTORIVER = one + two;
        $.SaveForm({
            url: "../../JXGeoManage/TBL_AVALANCHE_HIDDEN/SaveForm?keyValue=" + keyValue,
            param: postData,
            loading: "正在保存数据...",
            success: function (data) {
                if ($("#MultMedia").attr("src") != "") {
                    $("#MultMedia")[0].contentWindow.SaveFileInfo(data.resultdata);
                }
                try {
                    getInfoTop().learun.currentIframe().$('#gridTable').trigger('reloadGrid');
                } catch (e) {
                    window.parent.$('#gridTable').trigger('reloadGrid');
                }
            }
        })
    }
