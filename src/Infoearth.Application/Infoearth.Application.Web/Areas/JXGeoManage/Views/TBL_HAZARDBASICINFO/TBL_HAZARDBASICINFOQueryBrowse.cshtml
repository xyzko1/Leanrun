                break;
            case "崩塌":
                url = "/JXGeoManage/TBL_AVALANCHE/TBL_AVALANCHEForm";
                break;
            case "泥石流":
                url = "/JXGeoManage/TBL_DEBRISFLOW/TBL_DEBRISFLOWForm";
                break;
            case "地面沉降":
                url = "/JXGeoManage/TBL_LANDSETTLEMENT/TBL_LANDSETTLEMENTForm";
